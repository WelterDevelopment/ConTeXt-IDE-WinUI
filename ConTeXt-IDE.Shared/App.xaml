				<ResourceDictionary x:Key="Default">
					
					<ResourceDictionary.MergedDictionaries>
						<!--<ColorPaletteResources  Accent="ForestGreen"  BaseHigh="Yellow" BaseLow="Orange" BaseMedium="Red" BaseMediumHigh="Violet" BaseMediumLow="Blue" />-->
